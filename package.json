{
  "name": "qalendar",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && tsc --emitDeclarationOnly",
    "prepare": "npm run build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "test:docker": "docker-compose up --build",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint qalendar --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "seed": "node src/development/faker/seed.js",
	"docs:dev": "vitepress dev docs",
	"docs:build": "vitepress build docs",
	"docs:serve": "vitepress serve docs --port=8080"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/qalendar.umd.js",
  "module": "./dist/qalendar.es.js",
  "exports": {
    ".": {
      "import": "./dist/qalendar.es.js",
      "require": "./dist/qalendar.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^5.1.0",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.17.10",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^9.5.4",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "sass": "^1.51.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vitepress": "^0.22.4",
    "vitest": "^0.9.3",
    "vue-tsc": "^0.34.7"
  }
}
